{
  "name": "@lightelligence/lightelligence-react",
  "version": "0.0.1",
  "private": true,
  "main": "dist/bundle.umd.js",
  "module": "es6/index.js",
  "scripts": {
    "start": "styleguidist server",
    "build:styleguide": "styleguidist build",
    "build": "rm -rf dist && webpack --config webpack.config.js",
    "build:inc": "rm -rf dist && webpack --config webpack.config.js",
    "pretest": "npm run lint",
    "test": "node scripts/test.js --env=jsdom --coverage",
    "test:watch": "node scripts/test.js --env=jsdom",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/ && prettier --write src/**/*.js",
    "transpile-commonjs": "babel src -d commonjs --copy-files --presets @babel/preset-env",
    "transpile-es6": "babel src -d es6 --copy-files"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.2.0",
    "@lightelligence/lightelligence-ui": "bitbucket:lightelligence/styleguide#0.1.1",
    "classnames": "^2.2.6",
    "humps": "^2.0.1",
    "uniqid": "^5.0.3",
    "url-polyfill": "^1.1.3"
  },
  "peerDependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/runtime": "^7.2.0",
    "autoprefixer": "7.1.6",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "commander": "^2.19.0",
    "css-loader": "0.28.7",
    "cypress-testing-library": "^2.2.1",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^3.0.1",
    "fs": "0.0.1-security",
    "fs-extra": "3.0.1",
    "glob": "^7.1.3",
    "html-webpack-plugin": "2.29.0",
    "husky": "^1.1.1",
    "isomorphic-style-loader": "^4.0.0",
    "jest": "20.0.4",
    "lint-staged": "^7.3.0",
    "loader-utils": "^1.2.3",
    "material-design-icons-iconfont": "^4.0.3",
    "node-sass": "^4.9.3",
    "normalize.css": "^8.0.0",
    "object-assign": "4.1.1",
    "postcss": "^7.0.8",
    "postcss-copy": "^7.1.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "^3.0.0",
    "postcss-svg": "^2.4.0",
    "prettier": "^1.14.2",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.6.0",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "react-styleguidist": "^7.3.8",
    "react-testing-library": "^5.1.0",
    "resolve": "1.6.0",
    "rsyncwrapper": "^3.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.3",
    "svg-url-loader": "^2.3.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "typeface-overpass-mono": "0.0.54",
    "typeface-pt-sans": "0.0.54",
    "url-loader": "0.6.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix -- --max-warnings 0",
      "prettier --write",
      "git add"
    ]
  }
}
