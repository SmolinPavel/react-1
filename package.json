{
  "name": "@lightelligence/lightelligence-react",
  "version": "0.0.3",
  "private": true,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "start": "styleguidist server",
    "build:styleguide": "styleguidist build",
    "build": "rm -rf dist && npm run transpile-esm && npm run transpile-cjs",
    "pretest": "npm run lint",
    "test:watch": "node scripts/test.js --env=jsdom",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/ && prettier --write src/**/*.js",
    "transpile-esm": "BABEL_ENV=esm babel src -d dist/esm --copy-files",
    "transpile-cjs": "BABEL_ENV=cjs babel src -d dist/cjs --copy-files ",
    "test": "react-scripts test --coverage"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.2.0",
    "@lightelligence/lightelligence-ui": "bitbucket:lightelligence/styleguide#0.1.1",
    "classnames": "^2.2.6",
    "humps": "^2.0.1",
    "uniqid": "^5.0.3",
    "url-polyfill": "^1.1.3"
  },
  "peerDependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/runtime": "^7.2.0",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "css-loader": "0.28.7",
    "cypress-testing-library": "^2.2.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.5.0",
    "file-loader": "^3.0.1",
    "husky": "^1.1.1",
    "lint-staged": "^7.3.0",
    "node-sass": "^4.9.3",
    "normalize.css": "^8.0.0",
    "object-assign": "4.1.1",
    "postcss": "^7.0.8",
    "postcss-loader": "^3.0.0",
    "postcss-svg": "^2.4.0",
    "prettier": "^1.14.2",
    "react": "^16.6.0",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "react-styleguidist": "^7.3.8",
    "react-testing-library": "^5.1.0",
    "resolve": "1.6.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.3",
    "svg-url-loader": "^2.3.2",
    "typeface-overpass-mono": "0.0.54",
    "typeface-pt-sans": "0.0.54",
    "url-loader": "0.6.2",
    "whatwg-fetch": "2.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.js": [
      "npm run lint:fix -- --max-warnings 0",
      "prettier --write",
      "git add"
    ]
  }
}
