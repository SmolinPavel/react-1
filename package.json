{
  "name": "@lightelligence/react",
  "version": "0.3.1",
  "license": "UNLICENSED",
  "main": "dist/cjs/index.js",
  "scripts": {
    "start": "styleguidist server",
    "build": "rm -rf dist && npm run transpile-esm && npm run transpile-cjs",
    "build:docs": "styleguidist build",
    "pretest": "npm run lint",
    "test": "echo 'no tests yet'",
    "test:watch": "react-scripts test --env=jsdom",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/ && prettier --write src/**/*.js",
    "transpile-esm": "BABEL_ENV=esm babel src -d dist/esm --copy-files",
    "transpile-cjs": "BABEL_ENV=cjs babel src -d dist/cjs --copy-files "
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.2.0",
    "@lightelligence/lightelligence-ui": "^0.3.3",
    "classnames": "^2.2.6",
    "humps": "^2.0.1",
    "react-uid": "^2.2.0",
    "uniqid": "^5.0.3",
    "url-polyfill": "^1.1.3"
  },
  "peerDependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/runtime": "^7.2.0",
    "babel-eslint": "^10.0.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "css-loader": "0.28.7",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-react-app": "^3.0.7",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-loader": "^2.1.2",
    "babel-loader": "^8.0.5",
    "eslint": "^5.14.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "fs": "0.0.1-security",
    "fs-extra": "3.0.1",
    "glob": "^7.1.3",
    "husky": "^1.1.1",
    "lint-staged": "^7.3.0",
    "normalize.css": "^8.0.0",
    "object-assign": "4.1.1",
    "prettier": "^1.14.2",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-dev-utils": "^5.0.1",
    "react-router-dom": "^4.3.1",
    "react-styleguidist": "^9.0.1",
    "react-testing-library": "^5.1.0",
    "resolve": "1.6.0",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.3",
    "svg-url-loader": "^2.3.2",
    "typeface-overpass-mono": "0.0.54",
    "typeface-pt-sans": "0.0.54",
    "url-loader": "0.6.2",
    "webpack": "^4.29.5",
    "whatwg-fetch": "2.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix -- --max-warnings 0",
      "prettier --write",
      "git add"
    ]
  }
}
